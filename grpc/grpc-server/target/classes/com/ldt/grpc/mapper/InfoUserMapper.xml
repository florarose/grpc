    <select id="findInfoUser"  resultType="string"  parameterType="int">
        select name from info_user where id = #{id}
    </select>
    <insert id="insertUser" parameterType="string">
       INSERT INTO `demo`.`info_user` (`name`)  VALUES ( #{name})
    </insert>
</mapper>
